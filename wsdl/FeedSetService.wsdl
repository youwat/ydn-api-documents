            </sequence>
          </extension>
        </complexContent>
      </complexType>
      <complexType name="FeedSetOperation">
        <complexContent>
          <extension base="tns:Operation">
            <sequence>
              <element maxOccurs="1" minOccurs="1" name="accountId" type="xsd:long"/>
              <element maxOccurs="unbounded" minOccurs="1" name="operand" type="tns:FeedSet"/>
