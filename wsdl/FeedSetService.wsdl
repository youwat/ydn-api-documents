        </sequence>
      </complexType>
      <complexType name="FeedSetPage">
        <complexContent>
          <extension base="adv:Page">
            <sequence>
              <element maxOccurs="unbounded" minOccurs="0" name="values" type="tns:FeedSetValues"/>
            </sequence>
          </extension>
        </complexContent>
      </complexType>
      <complexType name="FeedSetValues">
        <complexContent>
          <extension base="adv:ReturnValue">
